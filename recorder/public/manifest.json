{
  "name": "OpenActionData",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {},
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "css": [],
      "js": [
        "js/inject.js"
      ]
    }
  ]
}
